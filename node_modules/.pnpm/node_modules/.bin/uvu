#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/AstroCompress/AstroCompress/node_modules/.pnpm/uvu@0.5.6/node_modules/uvu/node_modules:/home/runner/work/AstroCompress/AstroCompress/node_modules/.pnpm/uvu@0.5.6/node_modules:/home/runner/work/AstroCompress/AstroCompress/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/AstroCompress/AstroCompress/node_modules/.pnpm/uvu@0.5.6/node_modules/uvu/node_modules:/home/runner/work/AstroCompress/AstroCompress/node_modules/.pnpm/uvu@0.5.6/node_modules:/home/runner/work/AstroCompress/AstroCompress/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uvu/bin.js" "$@"
else
  exec node  "$basedir/../uvu/bin.js" "$@"
fi
